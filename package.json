{
  "name": "sample-project",
  "version": "1.0.0",
  "description": "A sample project for updating package.json",
  "main": "index.js",
  "module": "index.esm.js",
  "scripts": {
    "start": "node index.js",
    "update-patches": "npm-check-updates -u -t patch && npm install",
    "update-minor": "npm-check-updates -u -t minor && npm install",
    "update-major": "npm-check-updates -u -t major && npm install",
    "browserslist": "npx browserslist"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.11",
    "node-fetch": "^2.6.6",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-sass": "^5.0.0",
    "gulp-terser": "^2.0.1",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "npm-check-updates": "^12.1.1"
  },
  "engines": {
    "node": ">=10",
    "browserslist": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "last 2 Edge versions"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sample-project.git"
  },
  "license": "MIT",
  "author": "John Doe",
  "contributors": [
    "Jane Smith",
    "Michael Johnson"
  ],
  "homepage": "https://github.com/sample-project",
  "bugs": {
    "url": "https://github.com/sample-project/issues",
    "email": "bugs@sample-project.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "private": false,
  "files": [
    "src/**/*",
    "dist/**/*",
    "index.js"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "optionalDependencies": {
    "lodash": "^4.17.21"
  },
  "keywords": [
    "sample",
    "project",
    "package.json",
    "npm"
  ]
}
